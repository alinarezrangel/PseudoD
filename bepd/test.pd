utilizar entsal.pseudo
utilizar cadena.pseudo
utilizar mate.pseudo
utilizar plugins.pseudo
utilizar tipos.pseudo
utilizar clases.pseudo
utilizar algor.pseudo

clase Padre hereda Objeto
	metodo foo
	metodo estatico bar
finclase

metodo Padre#foo con yo y otro
	escribir yo
	escribir {: }
	escribir otro
	nl
finmetodo

metodo estatico Padre#bar con otro
	escribir {Padre#bar: }
	escribir otro
	nl
finmetodo

clase Hijo hereda Padre
finclase

instancia Padre pp
instancia Hijo pq

Importar.PseudoD.debug
