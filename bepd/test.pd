utilizar entsal.pseudo

clase A
	:hola
finclase
heredar Objeto A

funcion A#hola
	adquirir yo
	recibir_parametro yo
	llamar ImprimirFormato {Hola desde %\k} yo finargs
	liberar yo
finfun

clase B
	:crear
	:destruir
	:hola
finclase
heredar A B

funcion B#crear
	adquirir yo
	recibir_parametro yo
	llamar ImprimirFormato {Creando B en %\k} yo finargs
	liberar yo
finfun

funcion B#destruir
	adquirir yo
	recibir_parametro yo
	llamar ImprimirFormato {Destruyendo B en %\k} yo finargs
	liberar yo
finfun

funcion B#hola
	adquirir yo
	recibir_parametro yo
	llamar ImprimirFormato {Hola B desde %\k} yo finargs
	liberar yo
finfun

instancia A _a
instancia B _b

llamar _a#hola finargs
llamar _b#hola finargs

liberar _a
liberar _b
